ate of reliable communication.", "embedding": [0.008662793, 0.008927345, 0.06698256, 0.018147703, -0.03448664, -0.03934605, -0.017284865, -0.009706941, -0.042768788, 0.10284654, 0.092082545, 0.005911887, 0.008788531, -0.017090518, -0.0309277, -0.02724198, -0.030878296, -0.06247975, -0.058906708, -0.013994858, 0.044366054, -0.028086664, -0.07197873, -0.0049530617, -0.04438628, 0.015618476, 0.022889007, 0.02038043, 0.10362212, 0.04982995, 0.00845398, 0.0038653358, 0.03794305, -0.06935053, -0.009530792, -0.0273545, 0.03916736, -0.005638618, -0.03061254, -0.038414843, 0.03929624, 0.011590849, 0.047495395, -0.054085877, -0.07249661, 0.0070741395, 0.026821664, 0.0027130255, 0.0010321687, -0.0704225, -0.012853736, 0.016677754, 0.039316937, -0.026344953, 0.013892546, -0.005453848, 0.0062105753, 0.031211328, -0.021378933, -0.016346933, -0.0017792232, -0.011980276, 0.033036735, -0.0539566, 0.0063608545, 0.037960976, -0.010630237, 0.018139454, -0.022855658, -0.010083832, -0.0134982625, -0.015394653, -0.05628169, -0.03019746, -0.027036019, 0.03787063, 0.022936033, 0.042158153, -0.04801948, 0.028990658, -4.1906915e-05, 0.017890532, 0.012110776, -0.01983971, -0.002659023, -0.0018539343, 0.03794669, -0.008466952, -0.03320847, -0.017954843, -0.016553914, 0.02764953, -0.016751053, 0.014116652, 0.01772926, 0.0052448735, -0.0058910577, 0.019462194, -0.015789969, 0.008098523, 0.01352769, 0.088689946, -0.015030512, 0.059263155, 0.0010237365, 0.0021606758, 0.0034507026, 0.022268493, -0.07375938, 0.03270442, -0.011155545, -0.020465707, 0.004329241, -0.034455664, 0.007857694, 0.028349929, 0.01780327, 0.0018002805, -0.07130223, 0.011331325, 0.022891307, 0.017028129, 0.037450895, 0.021388028, -0.011181654, 0.0017922127, 0.01692661, 0.0022656426, -0.026137955, -0.015091816, -0.013007071, 0.016616737, -0.023106642, 0.009734164, -0.0068273577, 0.006410086, 0.014250373, 0.0037151151, 0.0016962382, -0.014616489, 0.027209682, -0.0304112, -0.017533744, 0.042781923, -0.020445956, 0.037741438, 0.0016