at I(N) =\n\nmaxpU;XI(U;Y).", "embedding": [0.0270134, 0.011998246, 0.02417501, 0.022474146, -0.025297662, -0.043955237, -0.038078, -0.0056895167, -0.05255815, 0.07990032, 0.08733644, 0.014988658, 0.014555044, 0.012368575, -0.029774446, -0.0052364515, -0.017563172, -0.015812842, -0.01569127, -0.004972349, 0.056010094, 0.01785493, -0.07135094, 0.005200114, -0.071065895, 0.013211827, 0.0016308648, 0.006966109, 0.059471767, 0.067442514, 0.035218447, 0.02938301, 0.024834927, -0.060253754, 0.0008174612, -0.024776923, 0.054574057, 0.01501002, -0.022002794, -0.025423659, 0.033731394, 0.020089807, 0.03422264, -0.053843483, -0.050963167, -0.014656915, -0.04049276, -0.0028551563, 0.012710864, -0.061792366, 0.0017503549, 0.019981816, 0.083475865, -0.032892313, 0.007616511, -0.02321488, 0.0053587016, 0.009010915, 0.016309723, 0.030889405, -0.03529743, -0.035078537, 0.0155922575, -0.083299905, -0.00062547444, 0.018830653, 0.0022558346, 0.014319862, -0.03920986, -0.0219993, 0.008217211, -0.032725282, -0.08300329, -0.0119198095, -0.008629053, 0.0442962, 0.014459127, 0.0061870785, -0.04391308, 0.019166466, 0.010674805, -0.036280096, -0.04677618, 0.017072728, 0.00016952248, -0.024190689, 0.03842343, -0.04155006, -0.014113342, 0.017920295, 0.016247848, 0.00990629, -0.032491773, 0.012738753, 0.013859388, 0.022726798, 0.018810444, 0.026001358, 0.002930161, 0.037044656, 0.044092596, 0.020596432, -0.023052, 0.061927445, 0.031544227, 0.056446783, 0.0006780219, 0.03917165, -0.018724011, -0.010910532, 0.012375635, -0.023110857, 0.025671378, -0.005782295, -0.003848131, -0.0010234802, 0.012038643, 0.017192101, -0.0404116, -0.025586432, 0.015326617, 0.008148588, 0.038854722, 0.022961685, 0.0020486338, -0.032646373, 0.012120718, -0.012180896, -0.043705456, -0.020581642, -0.009892535, 0.0072092265, -0.019026246, 0.013762837, -0.01344854, -0.0013173429, 0.005184058, 0.006482691, -0.002687838, 0.011574562, 0.0016790783, -0.038936865, -0.0002770392, 0.05283146, 0.013978269, 0.043594267, -0.03211418