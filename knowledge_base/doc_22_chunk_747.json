ue of jAj).", "embedding": [0.0023346553, -0.017860837, 0.014068729, 0.0031355573, -0.03797014, -0.045145985, -0.0075165723, -0.021275152, -0.030471578, 0.04601246, 0.0653332, -0.0036234497, -0.008318705, -0.021872154, -0.0060287067, 0.024094276, -0.043002542, -0.010850009, -0.047197077, 0.0029735034, 0.03795348, -0.02313163, -0.08229565, -0.0017847015, -0.032951906, -0.00557396, 0.02390808, 0.018617941, 0.08378387, 0.073575735, 0.02969791, -0.026335757, 0.003551283, -0.028686691, 0.01619458, 0.001223387, 0.038163144, 0.016921122, 0.0023638883, -0.044640485, 0.023224529, 0.04683165, 0.013636584, -0.045028888, -0.0521905, -0.045530725, -0.04024042, -0.0026389249, 0.034121305, -0.06145782, -0.049017537, -0.0051571284, 0.041343197, -0.054918244, 0.005943698, 0.0064711813, -0.005427844, 0.032323424, -0.020077769, 0.03209688, 0.04360826, -0.005502826, 0.035670124, -0.04910034, 0.029271495, 0.017094977, -0.019750563, 0.018322619, -0.019718004, -0.006973981, 0.033446863, -0.026986947, -0.036603224, -0.02887526, -0.045531508, 0.032040976, -0.01401847, 0.0131300185, -0.041524738, -0.018298676, 0.022901054, -0.019217456, -0.015863914, -0.0023586096, 0.011225465, 0.009559984, 0.03034164, -0.03860609, -0.013435247, -0.027332788, 0.016946377, 0.05730789, -0.015450194, -0.0069418433, 0.004349657, 0.019597063, -0.0069074943, 0.03669424, 0.029502537, 0.041499767, 0.0036495693, 0.04328493, 0.0188145, 0.0825311, 0.00865673, 0.025477981, -0.013800788, 0.016374173, -0.026922872, -0.03434827, 0.024605617, -0.012884412, -0.014362611, 0.012854784, -0.011554396, 0.032810044, -0.013138815, 0.011945712, -0.014450322, -0.029316958, -0.0041085714, -0.0008838912, 0.036360182, 0.018853344, -0.01196908, -0.029031467, 0.00740904, -0.030422857, -0.042407356, -0.02734706, 0.004808168, 0.012261634, 0.0034200018, -0.006780389, -0.020408655, 0.014000718, 0.004256777, 0.020526009, -0.03006955, 0.0020578026, 0.04633859, -0.048865624, 0.024110414, 0.02296871, -0.018816167, 0.035898406, 0.013771735, 0.0053