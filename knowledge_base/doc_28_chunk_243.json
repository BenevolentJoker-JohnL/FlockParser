uster states and the computa-\n\ntional power they o\ufb00er.", "embedding": [0.020968147, -0.02233974, -0.006900972, -0.012023757, -0.006370618, -0.02939165, -0.021646513, -0.018743437, -0.059759274, 0.085577354, 0.077922784, 0.007783962, 0.025209473, -0.069100924, -0.040858205, -0.015963724, -0.011950119, 0.0025854371, -0.027934281, 0.0033475899, 0.027145829, -0.005043373, -0.10389047, -0.0046300855, -0.063420236, 0.015088812, 0.03270983, 0.042190526, 0.050745, 0.022873387, 0.016507383, -0.016223297, 0.030950544, -0.012194846, -0.0112977335, -0.01007196, 0.028781146, -0.011099418, -0.0002849435, -0.028161906, 0.021989603, -0.00045697714, 0.03897807, -0.054027464, -0.032782037, -0.01240842, -0.039302777, 0.0125917075, 0.0016569329, -0.10698066, -0.047786932, -0.0102434065, 0.02601662, -0.0007951638, 0.023488792, -0.005159872, 0.007850662, 0.0044815894, -0.0060379338, 0.0366177, 0.02463432, -0.0017118481, 0.045612946, -0.07965065, 0.010375174, 0.018254142, 0.008513154, -0.007849128, 0.0006292261, -0.0727299, 0.0013410118, -0.06710393, -0.037786897, -0.014233512, -0.03581012, 0.03691423, 0.0024119073, -0.0048194877, -0.06315141, -0.0030714993, 0.018959071, -0.056490198, -0.045587163, -0.014408291, -0.020020809, -0.021338213, 0.00803533, -0.018173132, 0.017555295, -0.0011669248, -0.0055416417, 0.03506387, -0.0054960637, -0.0026096778, 0.058366507, 0.031518087, 0.014700302, 0.017959774, 0.021263273, -0.015190317, -0.017342845, 0.018432619, 0.02471223, 0.061836563, -0.019322028, 0.05575451, -0.013436119, 0.044989575, 0.015465845, -0.03855923, -0.018576454, -0.016840598, -0.012276977, 0.03556424, 0.007166673, 0.003232954, 0.0017695917, -0.015768968, -0.042725395, 0.0066698818, 0.020335019, -0.008045638, 0.07522145, 0.058219384, 0.020016706, -0.019503916, 0.034329783, -0.0120691145, -0.02492368, -0.048130512, 0.03835023, -0.009282564, -0.024092605, 0.020515107, -0.03382369, 0.010102158, -0.006328681, 0.039730743, -0.024035087, 0.011093256, 0.033498082, -0.015143115, 0.015